class CreateComponent {
    constructor(data, storeService) {
        this.data = data;
        this.storeService = storeService;
        this.minDate = new Date();
        this.disabled = true;
        this.addTodoForm = new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormGroup"]({
            title: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"](),
            deadline: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"](),
            priority: new _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormControl"](),
        });
    }
    ngOnInit() {
        this.addTodoForm.statusChanges.subscribe((status) => {
            this.disabled = status !== 'VALID';
        });
    }
    add() {
        if (this.addTodoForm.status === 'VALID') {
            this.addTodoForm.controls.deadline.setValue((this.addTodoForm.value.deadline.toISOString()).split('T')[0]);
            this.storeService.addTodoList(this.addTodoForm.value);
        }
        else {
            alert('Form does not valid');
        }
    }
